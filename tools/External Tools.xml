<toolSet name="External Tools">
  <tool name="show byte code" description="查看字节码" showInMainMenu="false" showInEditor="false" showInProject="false" showInSearchPopup="false" disabled="false" useConsole="true" showConsoleOnStdOut="false" showConsoleOnStdErr="false" synchronizeAfterRun="true">
    <exec>
      <option name="COMMAND" value="$JDKPath$/bin/javap" />
      <option name="PARAMETERS" value="-c $FileNameWithoutAllExtensions$.class" />
      <option name="WORKING_DIRECTORY" value="$OutputPath$//$FileDirRelativeToSourcepath$" />
    </exec>
  </tool>
</toolSet>